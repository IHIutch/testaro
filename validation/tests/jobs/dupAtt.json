{
  "id": "dupAtt",
  "what": "validation of dupAtt test",
  "strict": true,
  "timeLimit": 20,
  "acts": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/dupAtt/good.html",
      "what": "page without duplicate attributes"
    },
    {
      "type": "test",
      "which": "testaro",
      "what": "elements with duplicate attributes",
      "withItems": true,
      "expect": [
        [
          "result.rules.dupAtt.data.total",
          "=",
          0
        ],
        [
          "result.rules.dupAtt.data.items.1"
        ]
      ],
      "rules": [
        "y",
        "dupAtt"
      ]
    },
    {
      "type": "url",
      "which": "__targets__/dupAtt/bad.html",
      "what": "page with duplicate attributes"
    },
    {
      "type": "test",
      "which": "testaro",
      "what": "elements with duplicate attributes",
      "withItems": true,
      "expect": [
        [
          "result.rules.dupAtt.data.total",
          "=",
          2
        ],
        [
          "result.rules.dupAtt.data.items.0",
          "e",
          {
            "tagName": "P",
            "attributes": [
              "class",
              "id",
              "class"
            ]
          }
        ],
        [
          "result.rules.dupAtt.data.items.1",
          "e",
          {
            "tagName": "BUTTON",
            "attributes": [
              "aria-label",
              "aria-label"
            ]
          }
        ]
      ],
      "rules": [
        "y",
        "dupAtt"
      ]
    }
  ],
