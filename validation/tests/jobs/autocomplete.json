{
  "id": "autocomplete",
  "what": "validation of autocomplete test",
  "strict": true,
  "timeLimit": 20,
  "acts": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/autocomplete/good.html",
      "what": "page with correct autocomplete attributes"
    },
    {
      "type": "test",
      "which": "testaro",
      "withItems": true,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          0
        ],
        [
          "standardResult.instances.length",
          "=",
          0
        ]
      ],
      "rules": [
        "y",
        "autocomplete"
      ]
    },
    {
      "type": "test",
      "which": "testaro",
      "withItems": false,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          0
        ],
        [
          "standardResult.instances.length",
          "=",
          0
        ]
      ],
      "rules": [
        "y",
        "autocomplete"
      ]
    },
    {
      "type": "url",
      "which": "__targets__/autocomplete/bad.html",
      "what": "page with incorrect autocomplete attributes"
    },
    {
      "type": "test",
      "which": "testaro",
      "withItems": true,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          4
        ],
        [
          "standardResult.instances.1.what",
          "i",
          "value family-name"
        ],
        [
          "standardResult.instances.1.tagName",
          "=",
          "INPUT"
        ],
        [
          "standardResult.instances.1.location.doc",
          "=",
          "dom"
        ],
        [
          "standardResult.instances.1.location.type",
          "=",
          "box"
        ],
        [
          "standardResult.instances.1.location.spec.height",
          ">",
          "15"
        ],
        [
          "standardResult.instances.2.excerpt",
          "=",
          "Your email address"
        ],
        [
          "standardResult.instances.2.ordinalSeverity",
          "=",
          2
        ]
      ],
      "rules": [
        "y",
        "autocomplete"
      ]
    },
    {
      "type": "test",
      "which": "testaro",
      "what": "autocomplete attributes for name and email",
      "withItems": false,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          4
        ],
        [
          "standardResult.instances.0.what",
          "i",
          "Inputs are missing"
        ],
        [
          "standardResult.instances.0.ordinalSeverity",
          "=",
          2
        ],
        [
          "standardResult.instances.0.count",
          "=",
          4
        ]
      ],
      "rules": [
        "y",
        "autocomplete"
      ]
    }
  ],
