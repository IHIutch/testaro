{
  "id": "radioSetVal",
  "what": "validation of radioSet test",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/radioSet/good.html",
      "what": "page with standard radio-button grouping"
    },
    {
      "type": "test",
      "which": "radioSet",
      "what": "radioSet",
      "withItems": false,
      "expect": [
        ["totals.total", "=", 4],
        ["totals.inSet", "=", 4],
        ["totals.percent", "=", 100]
      ]
    },
    {
      "type": "url",
      "which": "__targets__/radioSet/bad.html",
      "what": "page with deviant radio-button grouping"
    },
    {
      "type": "test",
      "which": "radioSet",
      "what": "radioSet",
      "withItems": false,
      "expect": [
        ["totals.total", "=", 8],
        ["totals.inSet", "=", 0],
        ["totals.percent", "=", 0]
      ]
    }
  ],
