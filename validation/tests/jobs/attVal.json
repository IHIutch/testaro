{
  "id": "attVal",
  "what": "validation of attVal test",
  "strict": true,
  "timeLimit": 20,
  "acts": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/attVal/good.html",
      "what": "page with permitted attribute values"
    },
    {
      "type": "test",
      "which": "testaro",
      "attributeName": "lang",
      "areLicit": true,
      "values": [
        "en-US",
        "de-CH"
      ],
      "what": "attribute values",
      "withItems": true,
      "expect": [
        [
          "result.rules.attVal.data.total",
          "=",
          0
        ],
        [
          "result.rules.attVal.data.items.1"
        ]
      ],
      "rules": [
        "y",
        "attVal"
      ]
    },
    {
      "type": "url",
      "which": "__targets__/attVal/bad.html",
      "what": "page with permitted attribute values"
    },
    {
      "type": "test",
      "which": "testaro",
      "attributeName": "lang",
      "areLicit": false,
      "values": [
        "en",
        "de"
      ],
      "what": "attribute values",
      "withItems": true,
      "expect": [
        [
          "result.rules.attVal.data.total",
          "=",
          2
        ],
        [
          "result.rules.attVal.data.items.0.tagName",
          "=",
          "HTML"
        ],
        [
          "result.rules.attVal.data.items.0.attributeValue",
          "=",
          "en"
        ],
        [
          "result.rules.attVal.data.items.1.tagName",
          "=",
          "SPAN"
        ],
        [
          "result.rules.attVal.data.items.1.attributeValue",
          "=",
          "de"
        ],
        [
          "result.rules.attVal.data.items.1.textStart",
          "i",
          "Veloparkieren"
        ]
      ],
      "rules": [
        "y",
        "attVal"
      ]
    }
  ],
