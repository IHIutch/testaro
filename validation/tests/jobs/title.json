{
  "id": "titleVal",
  "what": "validation of title test",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/title/good.html",
      "what": "page with a title"
    },
    {
      "type": "test",
      "which": "title",
      "what": "document title",
      "expect": [
        ["title", "=", "Test page"]
      ]
    },
    {
      "type": "url",
      "which": "__targets__/title/bad.html",
      "what": "page without a title"
    },
    {
      "type": "test",
      "which": "title",
      "what": "document title",
      "expect": [
        ["title", "=", ""]
      ]
    }
  ],
  "sources": {
    "script": "",
    "host": {},
    "requester": ""
  },
  "jobCreationTime": "",
  "timeStamp": ""
}
