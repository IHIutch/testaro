{
  "id": "title",
  "what": "validation of title test",
  "strict": true,
  "timeLimit": 20,
  "acts": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/title/good.html",
      "what": "page with a title"
    },
    {
      "type": "test",
      "which": "testaro",
      "what": "document title",
      "stopOnFail": true,
      "expect": [
        [
          "result.rules.title.data.title",
          "=",
          "Test page"
        ]
      ],
      "withItems": true,
      "rules": [
        "y",
        "title"
      ]
    },
    {
      "type": "url",
      "which": "__targets__/title/bad.html",
      "what": "page without a title"
    },
    {
      "type": "test",
      "which": "testaro",
      "what": "document title",
      "stopOnFail": true,
      "expect": [
        [
          "result.rules.title.data.title",
          "=",
          ""
        ]
      ],
      "withItems": true,
      "rules": [
        "y",
        "title"
      ]
    }
  ],
